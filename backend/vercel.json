{
    "version": 2,
    "name": "save2740-backend",
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "iad1"
    ]
}